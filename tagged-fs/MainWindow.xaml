<Window x:Class="tagged_fs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tagged_fs"
        xmlns:res="clr-namespace:tagged_fs.Properties"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="1000">
    <Window.Resources>
        <local:GetFileSystemInformationConverter x:Key="FsConverter" />
        <local:FileSystemObject x:Key="ListViewDataSource"/>

        <HierarchicalDataTemplate DataType="{x:Type local:FileSystemObject}" ItemsSource="{Binding Converter={StaticResource FsConverter}}">
            <StackPanel Orientation="Horizontal" Cursor="Hand">
                <Image Width="20" Height="20" Stretch="Fill" x:Name="Image" />
                <TextBlock Text="{Binding Name}" Margin="5,0,0,0" />
            </StackPanel>
            <HierarchicalDataTemplate.Triggers>
                <DataTrigger Binding="{Binding Path=Type}" Value="0">
                    <Setter Property="Image.Source" TargetName="Image" Value="Resources/computer.png" />
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=Type}" Value="1">
                    <Setter Property="Image.Source" TargetName="Image" Value="Resources/drive.png" />
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=Type}" Value="2">
                    <Setter Property="Image.Source" TargetName="Image" Value="Resources/folder.png" />
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=Type}" Value="3">
                    <Setter Property="Image.Source" TargetName="Image" Value="Resources/file.png"/>
                </DataTrigger>
                <DataTrigger Binding="{Binding Path=Type}" Value="4">
                    <Setter Property="Image.Source" TargetName="Image" Value="Resources/tags.png"/>
                </DataTrigger>
            </HierarchicalDataTemplate.Triggers>
        </HierarchicalDataTemplate>

        <Style TargetType="TreeViewItem">
            <Setter Property="IsExpanded" Value="{Binding IsExpanded, Mode=TwoWay}" />
            <Setter Property="IsSelected" Value="{Binding IsSelected, Mode=TwoWay}" />
            <EventSetter Event="MouseLeftButtonUp" Handler="TreeViewItem_MouseLeftButtonUp"></EventSetter>
        </Style>

        <Style TargetType="ListViewItem">
            <EventSetter Event="MouseDoubleClick" Handler="ListViewItem_OnDoubleClick"></EventSetter>
        </Style>

        <Style x:Key="ListViewHeaderStyle" TargetType="{x:Type GridViewColumnHeader}">
            <Setter Property="HorizontalContentAlignment" Value="Left" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="Height" Value="30" />
        </Style>

        <DataTemplate x:Key="FolderDataTemplate" DataType="local:FileSystemObject">
            <StackPanel Orientation="Horizontal">
                <Image Width="20" Height="20" Stretch="Fill" x:Name="ExplorerImage" />
                <TextBlock Text="{Binding Name}" Margin="5,0,0,0" VerticalAlignment="Center" Foreground="Black" />
            </StackPanel>
            <DataTemplate.Triggers>
                <DataTrigger Binding="{Binding Type}" Value="1">
                    <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/drive.png" />
                </DataTrigger>
                <DataTrigger Binding="{Binding Type}" Value="2">
                    <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/folder.png" />
                </DataTrigger>
                <DataTrigger Binding="{Binding Type}" Value="3">
                    <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/file.png" />
                </DataTrigger>
            </DataTemplate.Triggers>
        </DataTemplate>

        <DataTemplate x:Key="ComputerDataTemplate" DataType="local:FileSystemObject">
            <StackPanel Orientation="Horizontal">
                <Image Width="20" Height="20" Stretch="Fill" x:Name="ExplorerImage" />
                <TextBlock Text="{Binding Extension}" Margin="5,0,0,0" VerticalAlignment="Center" Foreground="Black" />
                <TextBlock Text="{Binding Modified}" Foreground="DimGray" />
            </StackPanel>
            <DataTemplate.Triggers>
                <DataTrigger Binding="{Binding Type}" Value="1">
                    <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/drive.png" />
                </DataTrigger>
                <DataTrigger Binding="{Binding Type}" Value="2">
                    <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/folder.png" />
                </DataTrigger>
                <DataTrigger Binding="{Binding Type}" Value="3">
                    <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/file.png" />
                </DataTrigger>
            </DataTemplate.Triggers>
        </DataTemplate>
    </Window.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="180"></ColumnDefinition>
            <ColumnDefinition Width="3"></ColumnDefinition>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TreeView x:Name="FsTreeView" Grid.Column="0" Margin="0" Grid.Row="1" />
        <GridSplitter HorizontalAlignment="Stretch" Grid.Column="1" Grid.Row="1"/>
        <ListView x:Name="ExplorerView" Grid.Column="2" ItemsSource="{Binding Source={StaticResource ListViewDataSource}}" Grid.Row="1" >
            <ListView.Style>
                <Style TargetType="ListView">

                    <Style.Triggers>
                        <DataTrigger Binding="{Binding TemplateType}" Value="0" >
                            <Setter Property="View">
                                <Setter.Value>
                                    <GridView>
                                        <GridViewColumn Header="{x:Static res:Resources.ExplorerView_ColumnName}" HeaderContainerStyle="{StaticResource ListViewHeaderStyle}" Width="300">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate DataType="local:FileSystemObject">
                                                    <StackPanel Orientation="Horizontal">
                                                        <Image Width="20" Height="20" Stretch="Fill" x:Name="ExplorerImage" />
                                                        <TextBlock Text="{Binding Name}" Margin="5,0,0,0" VerticalAlignment="Center" Foreground="Black" />
                                                    </StackPanel>
                                                    <DataTemplate.Triggers>
                                                        <DataTrigger Binding="{Binding Type}" Value="1">
                                                            <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/drive.png" />
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding Type}" Value="2">
                                                            <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/folder.png" />
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding Type}" Value="3">
                                                            <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/file.png" />
                                                        </DataTrigger>
                                                    </DataTemplate.Triggers>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="{x:Static res:Resources.ExplorerView_ColumnTotalSize}" HeaderContainerStyle="{StaticResource ListViewHeaderStyle}" Width="150">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate DataType="local:FileSystemObject">
                                                    <TextBlock Text="{Binding TotalSize}" Foreground="DimGray" />
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="{x:Static res:Resources.ExplorerView_ColumnFreeSpace}" HeaderContainerStyle="{StaticResource ListViewHeaderStyle}" Width="150">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate DataType="local:FileSystemObject">
                                                    <TextBlock Text="{Binding FreeSpace}" Foreground="DimGray" />
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </Setter.Value>
                            </Setter>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding TemplateType}" Value="1">
                            <Setter Property="View">
                                <Setter.Value>
                                    <GridView>
                                        <GridViewColumn Header="{x:Static res:Resources.ExplorerView_ColumnName}" HeaderContainerStyle="{StaticResource ListViewHeaderStyle}"
                                                        Width="300" >
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate DataType="local:FileSystemObject">
                                                    <StackPanel Orientation="Horizontal">
                                                        <Image Width="20" Height="20" Stretch="Fill" x:Name="ExplorerImage" />
                                                        <TextBlock Text="{Binding Name}" Margin="5,0,0,0" VerticalAlignment="Center" Foreground="Black" />
                                                    </StackPanel>
                                                    <DataTemplate.Triggers>
                                                        <DataTrigger Binding="{Binding Type}" Value="1">
                                                            <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/drive.png" />
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding Type}" Value="2">
                                                            <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/folder.png" />
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding Type}" Value="3">
                                                            <Setter Property="Image.Source" TargetName="ExplorerImage" Value="Resources/file.png" />
                                                        </DataTrigger>
                                                    </DataTemplate.Triggers>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="{x:Static res:Resources.ExplorerView_ColumnModified}" HeaderContainerStyle="{StaticResource ListViewHeaderStyle}"
                                                        Width="150" >
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate DataType="local:FileSystemObject">
                                                    <TextBlock Text="{Binding Modified}" Foreground="DimGray" />
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="{x:Static res:Resources.ExplorerView_ColumnType}" HeaderContainerStyle="{StaticResource ListViewHeaderStyle}"
                                                        Width="100" >
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate DataType="local:FileSystemObject">
                                                    <TextBlock Text="{Binding Extension}" Foreground="DimGray" />
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn Header="{x:Static res:Resources.ExplorerView_ColumnSize}" HeaderContainerStyle="{StaticResource ListViewHeaderStyle}"
                                                        Width="100" >
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate DataType="local:FileSystemObject">
                                                    <TextBlock Text="{Binding Size}" Foreground="DimGray" />
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </Setter.Value>
                            </Setter>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </ListView.Style>
        </ListView>
